[{"_id":{"$objectID":"5aa944f207aa2f44d0222250"},"name":"Inter-University Transfer Details","description":"UI for displaying university transfer details slip","uid":"INTER_UNIVERSITY_DETAILS","template":"<style>\n    .section_template{\n        background-color:var(--color-background);\n    }\n\n    .section_template:after{\n        /*width:100%;*/\n    }\n    .section-header{\n        font-weight:bold;\n        text-align: center;\n        background-color:var(--color-primary);\n        color:var(--color-white);\n    }\n</style>\n\n<hr />\n<div class=\"row\">\n   <h3 class=\"section-header\">Transfer Information</h3> \n   <hr />\n</div>\n    \n<div class=\"row section_template\">\n    <div class=\"col-sm-12 col-xs-12 text-left\">\n        <div class=\"col-sm-6\">\n            <% for(var i=0; i < currentFaculty.length; i++) { %>\n                <strong><%= currentFaculty[i][0] %></strong>\n                <span><%= currentFaculty[i][1] %></span>\n                <br>\n            <% }%>\n            \n        </div>\n        <div class=\"col-sm-6\">\n            <% for(var i=0; i < proposedFaculty.length; i++) { %>\n                <strong><%= proposedFaculty[i][0] %></strong>\n                <span><%= proposedFaculty[i][1] %></span>\n                <br>\n            <% }%>\n        </div>\n    </div>\n</div>\n\n<hr />\n<div class=\"row\">\n   <h3 class=\"section-header\">Certificate's History</h3> \n   <hr />\n</div>\n<div class=\"row section_template\">\n    <table class=\"table table-reponsive table-hover\">\n        <thead>\n            <tr>\n                <td class=\"col-sm-3 col-xs-3\">Institution</td>\n                <td class=\"col-sm-3 col-xs-3\">From</td>\n                <td class=\"col-sm-3 col-xs-3\">To</td>\n                <td class=\"col-sm-2 col-xs-2\">Certificate Obtained</td>\n                <td class=\"col-sm-1 col-xs-1\">Grade</td>\n            </tr>\n        </thead>\n        <tbody>\n        <% certificateHistory[0][1].history.forEach(certificate => { %>\n            <tr>\n               <td class=\"col-sm-3 col-xs-3\"><%= certificate.institution %></td>\n               <td class=\"col-sm-3 col-xs-3\"><%= certificate.from %></td>\n               <td class=\"col-sm-3 col-xs-3\"><%= certificate.to %></td>\n               <td class=\"col-sm-2 col-xs-2\"><%= certificate.certificateObtained %></td>\n               <td class=\"col-sm-1 col-xs-1\"><%= certificate.grade %></td>\n            </tr>\n        <% }); %>\n        </tbody>\n    </table>\n</div>","domain":{"$objectID":"59b16c8fe258df1c5c7d5c25"},"__v":0,"updated":"2018-03-21T10:53:46.056Z"}]
